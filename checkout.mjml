<mjml>
    <mj-body background-color="#E4E6EB" font-size="16px">
        <mj-section background-color="#FFFFFF" vertical-align="top" padding-bottom="0px" padding-top="0px" border-bottom="1px dashed #eee">
            <mj-column vertical-align="top" width="100%">

                <mj-image src="images/logo@2x.png" alt="" align="center" border="none" width="180px" padding-left="0px" padding-right="0px" padding-bottom="25px" padding-top="30px"  />

            </mj-column>
        </mj-section>

        <mj-section background-color="#FFFFFF" vertical-align="top" padding-bottom="0px" padding-top="0px">
            <mj-column vertical-align="top" width="100%">
                <mj-text align="center" color="#071C4D" font-size="30px" font-weight="bold" font-family="Playfair Display, Times New Roman, serif" padding-left="25px" padding-right="25px" padding-top="30px" padding-bottom="30px">
                    Don't Forget Your Ticket
                </mj-text>
            </mj-column>
        </mj-section>

        <mj-section background-color="#FFFFFF" padding-bottom="0px" padding-top="0px">
            <mj-column vertical-align="top" width="100%">

                <mj-image src="images/nusa_penida@2x.jpg" alt="" align="center" border="none" width="550px" padding-left="0px" padding-right="0px" padding-bottom="25px" padding-top="30px"  />

            </mj-column>
        </mj-section>

        <mj-section background-color="#FFFFFF" padding-bottom="20px" padding-top="20px">
            <mj-column vertical-align="middle" width="100%" border-bottom="1px dashed #eee" padding-bottom="30px">
                <mj-text align="left" color="#071C4D" font-size="18px" line-height="20px" font-family="Assistant, Helvetica, Arial, sans-serif" padding-left="25px" padding-right="25px">
                    Hi, Zaedan
                    <br><br>
                    Tiket Digital Kamu Sudah Berhasil Dicetak.
                    <br>
                    Booking ID <strong>#22081996</strong>
                </mj-text>

                <mj-table color="#071C4D" font-size="18px" line-height="28px" font-family="Assistant, Helvetica, Arial, sans-serif" padding-left="25px" padding-right="25px">
                   <tr>
                        <td colspan="3">Members</td>
                    <tr>
                    <tr>
                        <td><strong>Zaedan</strong></td>
                        <td style="text-align:right;">Singapore, SG
                    <tr>
                    <tr>
                        <td><strong>Zidan</strong></td>
                        <td style="text-align:right;">Singapore, SG
                    <tr>
                </mj-table>

                <mj-table color="#071C4D" font-size="18px" line-height="28px" font-family="Assistant, Helvetica, Arial, sans-serif" padding-left="25px" padding-right="25px">
                   <tr>
                        <td colspan="3">Travel Details</td>
                    <tr>
                    <tr>
                        <td><strong>Departure</strong></td>
                        <td style="text-align:right;">Nusa Penida
                    <tr>
                    <tr>
                        <td><strong>Date and Time</strong></td>
                        <td style="text-align:right;">Thu, 20 Jan, 2023
                    <tr>
                </mj-table>

                <mj-button align="center" font-size="18px" font-weight="bold" background-color="#FF9E53" border-radius="10px" color="#FFF" font-family="Assistant, Helvetica, Arial, sans-serif">
                    Check Detail
                </mj-button>

            </mj-column>
        </mj-section>


    </mj-body>
</mjml>
